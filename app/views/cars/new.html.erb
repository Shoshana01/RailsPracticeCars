<h1>Add a car</h1>
<%= link_to "Back to Cars", cars_url %>

<%= form_for @car do |f| %>
    <p>
    <%= f.label :owner %><br>
    <%= f.text_field :owner %>
    </p>
    <p>
    <%= f.label :model %><br>
    <%= f.text_field :model %>
    </p>
    <p>
    <%= f.label :color %><br> 
    <%= f.text_field :color %>
    </p>
    <p>
    <%= f.submit "Save" %>
    </p>
    <%end%>