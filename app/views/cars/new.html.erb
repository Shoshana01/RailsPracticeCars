<h1>Add a car</h1>
<%= form_tag "/cars" do %>
    <p>
    <%= label_tag :owner %><br>
    <%= text_field_tag :owner %>
    </p>
    <p>
    <%= label_tag :model %><br>
    <%= text_field_tag :model %>
    </p>
    <p>
    <%= label_tag :color %><br> 
    <%= text_field_tag :color %>
    </p>
    <p>
    <%= submit_tag "Save" %>
    </p>
    <%end%>