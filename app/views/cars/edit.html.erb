<h1>Edit Car</h1>

<p>
    <%= link_to "Back", @car %>
 </p>

<%= form_for @car do |f| %>
    <p>
    <%= f.label :owner %><br>
    <%= f.text_field :owner %>
    </p>
    <p>
    <%= f.label :model %><br>
    <%= f.text_field :model %>
    </p>
    <p>
    <%= f.label :color %><br>
    <%= f.text_field :color %>
    </p>
    <p>
    <%= f.submit %>
    </p>
    <% end %>